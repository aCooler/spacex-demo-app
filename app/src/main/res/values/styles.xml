<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="card_rocket_name">
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:lineSpacingExtra">4.8sp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:textColor">@color/text_white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="card_place" parent="card_rocket_name">
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">3</item>
    </style>
    <style name="card_reused">
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:lineSpacingExtra">4.8sp</item>
        <item name="android:textColor">@color/text_white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:paddingLeft">8dp</item>
    </style>
    <style name="details">
        <item name="android:background">@color/basic_background</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:lineSpacingExtra">8sp</item>
        <item name="android:textColor">#fafafa</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_marginBottom">40dp</item>
        <item name="android:layout_marginTop">16dp</item>
    </style>
    <style name="gallery_viewpager">
        <item name="android:animationCache">false</item>
        <item name="android:background">@drawable/sky</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="row_row">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/text_white</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
    </style>
    <style name="single_single_title">
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:lineSpacingExtra">4.8sp</item>
        <item name="android:textColor">@color/text_white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="single_single_text">
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:background">@drawable/bottom_border</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:lineSpacingExtra">8sp</item>
        <item name="android:textColor">#fafafa</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="timer_title_style">
        <item name="android:backgroundTintMode">src_in</item>
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:foregroundTint">@color/real_white</item>
        <item name="android:foregroundTintMode">add</item>
        <item name="android:lineSpacingExtra">2.8sp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="rocket_name_style">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textColor">@color/real_white</item>
        <item name="android:textSize">30sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="bottom_timer_words">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:lineSpacingExtra">2.8sp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">@color/timer_numbers_color</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="numbers_timer_style">
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textColor">@color/real_white</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="launches_and_rocket_item_style">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textColor">@color/real_white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="mission_name_style_style" >
        <item name="android:layout_marginTop">32dp</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:lineSpacingExtra">4.8sp</item>
        <item name="android:textColor">@color/text_white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="launches_total_successful_style">
        <item name="android:layout_marginTop">11dp</item>
        <item name="android:background">@drawable/bottom_border</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:lineSpacingExtra">8sp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:textColor">#1aac80</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="launches_total_all_launches">
        <item name="android:contentDescription">@string/app_name</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:lineSpacingExtra">7sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="launches_total_tolaunch_style">
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:background">@drawable/frame_48</item>
        <item name="android:contentDescription">@string/app_name</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="launches_total_total_style">
        <item name="android:layout_marginTop">11dp</item>
        <item name="android:background">@drawable/bottom_border</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:lineSpacingExtra">8sp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="launches_total_efficiency_style">
        <item name="android:layout_marginTop">25dp</item>
        <item name="android:layout_marginBottom">32dp</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:lineSpacingExtra">8sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="rockets_start_efficiency_style">
        <item name="android:layout_marginTop">32dp</item>
        <item name="android:layout_marginBottom">64dp</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:lineSpacingExtra">8sp</item>
        <item name="android:textColor">@color/real_white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="elon_mask_style">
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:textStyle">italic</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/text_white</item>
        <item name="android:lineSpacingExtra">8sp</item>
    </style>
    <style name="rockets_start_explore_rockets_style">
        <item name="android:lineSpacingExtra">7sp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:contentDescription">@string/app_name</item>
    </style>
    <style name="rocket_start_torocket_style">
        <item name="android:background">@drawable/frame_48</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:contentDescription">@string/app_name</item>
    </style>
</resources>